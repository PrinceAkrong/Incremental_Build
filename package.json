{
  "name": "my-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "yarn process-markdown",
    "process-markdown": "processmd \"content/**/*.md\" --outputDir content --summaryOutput content/summary.json --includeBodyProps --headingIds",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "export": "npm run build && next export"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "next": "9.0.6",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "styled-components": "^4.3.2",
    "styled-icons": "^8.4.2",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "frontmatter-markdown-loader": "^2.1.0",
    "next-compose-plugins": "^2.2.0",
    "processmd": "^4.4.2"
  }
}
