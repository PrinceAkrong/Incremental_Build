  "compilerOptions": {
    "allowUnreachableCode": true,
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
  },
  "files": [
    "src/**/*"
  ],
  "exclude": [
    ".cache/**/*",
    "node_modules/**/*",
    "public/**/*"
  ]
}